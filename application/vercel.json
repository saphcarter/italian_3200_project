{
  "name": "italian-3200-project",
  "version": 2,
  "builds": [
    {
      "src": "src/main.jsx",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/results",
      "dest": "/results.jsx"
    },
    {
      "src": "/profile",
      "dest": "/profile.jsx"
    }
  ]
}
