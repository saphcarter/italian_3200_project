{
  "name": "italian-3200-project",
  "version": 2,
  "builds": [
    {
      "src": "vite.config.js",
      "use": "@vercel/node",
      "config": {
        "zeroConfig": true
      }
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "vite.config.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
