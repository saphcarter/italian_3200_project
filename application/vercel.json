{
  "name": "italian-3200-project",
  "version": 2,
  "builds": [
    {
      "src": "/main.jsx",
      "use": "@vercel/node"
    }
  ],
  "buildCommand": ["vite build", "npm run build"],
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "vite",
  "framework": "vite",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/results",
      "dest": "/src/results.jsx"
    },
    {
      "src": "/profile",
      "dest": "/src/profile.jsx"
    }
  ],
  "env": {
    "MY_ENV_VARIABLE": "my-value"
  }
}
